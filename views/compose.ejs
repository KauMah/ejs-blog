<%- include("partials/header"); %>
<h1>Compose</h1>
<form class="" action="/compose" method="POST">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle" />
    <label>Post</label>
    <textarea class="form-control" name="content" cols="50" rows="5"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="submitButton">
    Publish
  </button>
</form>
